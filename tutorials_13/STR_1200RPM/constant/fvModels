// Sparge holes
ang1 #calc "degToRad(22.5)";
ang2 #calc "degToRad(45)";
ang3 #calc "degToRad(67.5)";
spargeRad  0.0245;

x1 #calc "$spargeRad*cos($ang1)";
x1neg #calc "-1.0*$x1";
z1 #calc "$spargeRad*sin($ang1)";
z1neg #calc "-1.0*$z1";
x2 #calc "$spargeRad*cos($ang2)";
x2neg #calc "-1.0*$x2";
z2 #calc "$spargeRad*sin($ang2)";
z2neg #calc "-1.0*$z2";
x3 #calc "$spargeRad*cos($ang3)";
x3neg #calc "-1.0*$x3";
z3 #calc "$spargeRad*sin($ang3)";
z3neg #calc "-1.0*$z3";
x4 0.0;
z4 0.0245;
z4neg -0.0245;

    massSource2
    {
        type            massSource;
        phase liquid;
        cellZone
        {
            type            containsPoints;
            points          
(        (0.03 0.06 0)

);
        }

        massFlowRate 6.645e-3;
    }


    massSource
    {
        type            massSource;
        phase gas;
        cellZone
        {
            type            containsPoints;
            points          
(        ($x1 0.01 $z1)
        ($x2 0.01 $z2)
        ($x3 0.01 $z3)
        ($x4 0.01 $z4)
        ($x1neg 0.01 $z1)
        ($x2neg 0.01 $z2)
        ($x3neg 0.01 $z3)
        ($x1 0.01 $z1neg)
        ($x2 0.01 $z2neg)
        ($x3 0.01 $z3neg)
        ($x4 0.01 $z4neg)
        ($x1neg 0.01 $z1neg)
        ($x2neg 0.01 $z2neg)
        ($x3neg 0.01 $z3neg)
);
        }

        massFlowRate 6.645e-6;
    }

