FoamFile
{
    format      ascii;
    class       dictionary;
    object      fvConstraints;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

limitp
{
    type       limitPressure;

    min        1e4;
}

limitTgas
{
    type            limitTemperature;
    active          yes;
    cellZone        all;
    min             288;
    max             298;
    phase           gas;
}

limitTliquid
{
    type            limitTemperature;
    active          yes;
    cellZone        all;
    min             288;
    max             298;
    phase           liquid;
}




